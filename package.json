{
  "name": "hostel-mgt",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "watch": "npx tailwindcss -i ./src/theme/tailwind.css -o ./src/output.css --watch",
    "prepare:native": "npx cap sync && npx cap copy",
    "prepare:prod": "export NODE_ENV=production && yarn prepare:native",
    "prepare:dev": "yarn prepare:native",
    "sync": "yarn build && npx cap sync",
    "ios": "yarn sync && npx cap run ios --livereload-url=http://localhost:3003 --watch --no-build",
    "android": "yarn sync && npx cap run android --livereload-url=http://localhost:3003 --watch --no-build"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/app": "6.0.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/haptics": "6.0.2",
    "@capacitor/keyboard": "6.0.3",
    "@capacitor/status-bar": "6.0.2",
    "@ionic/react": "^8.0.0",
    "@ionic/react-router": "^8.0.0",
    "@ionic/storage": "^4.0.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "classnames": "^2.5.1",
    "ionicons": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "6.2.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "~5.2.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^0.34.6"
  },
  "description": "DoorApp - Final year project"
}
